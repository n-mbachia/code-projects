print("python --version")
print(3 + 4)
print(4 - 3)
print(3 * 4)
print(4 % 3)
print(4 / 3)
print( 3 ** 4)
print(4 // 3)
print()
print('Mbachia')
print('Ng\'ethe')
print('Kenya')
print('I am enjoyinmg 30 days of python')
print()
print(type(10))
print(type(9.8))
print(type(3.14))
print(type(4 - 4))
print(type(['Asabeneh', 'Python', 'Finland']))
print(type('Mbachia'))
print(type('Ng\'ethe'))
print(type('Kenya'))
print()
print(type(-10)) # int
print(type(452.125)) # float
print(type(1 +3j)) # complex
print(type('the milk is bad')) # string
print(type(['orange', 'yellow', 'red'])) # list
print(type(True)) # booleans
print(type(False)) # booleans
print(type({'city': 'Nairobi',
            'country': 'Kenya',
            'continent': 'Africa'}
           )) # dictionary
print(type(('Earth', 'moon', 'sun'))) # tuple
print(type({3.14, 6.87, 2, 1, 0}))
print()

