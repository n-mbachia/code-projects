#!/usrs/bin/python3
"""A dictionary is a collection of unordered. modifiable paired data type."""

dog = {}
dog['name'] = 'Bossy'
dog['breed'] = 'Mix of german sherpard and japanese Shitsu'
dog['legs'] = 4
dog['age'] = 5
print(dog)
print()
student = {
    'first_name': 'Daniel'
    "last_name":"Ng'ethe"
    'gender':'Male'
    'age':32
    'martial_status' : 'Single'
    'skills':'Boxing'
    'country':'Kenya'
    'city':'Ol Kalou'
    'address':{
        'box':62470
        'code':'00100'
        'city':'Nairobi'
        }
}
